{
  "name": "derek",
  "version": "0.0.2",
  "description": "Convert data structure to schema.",
  "directories": {
    "doc": "docs"
  },
  "main": "./js/src/index.cjs",
  "exports": {
    "import": "./js/src/wrapper.mjs",
    "require": "./js/src/index.cjs"
  },
  "files": [
    "js/src/*.mjs"
  ],
  "scripts": {
    "test": "jest --silent",
    "coverage": "yarn test --collectCoverage && istanbul-cobertura-badger -b js -d .badges/coverage"
  },
  "repository": "git+https://github.com/benjaminwoods/derek.git",
  "keywords": [
    "data",
    "schema",
    "convert",
    "analyze"
  ],
  "author": "Benjamin Woods",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjaminwoods/derek/issues"
  },
  "homepage": "https://github.com/benjaminwoods/derek#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "istanbul-cobertura-badger": "^1.3.1",
    "jest": "^27.4.5",
    "prettier": "^2.5.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "js/src/**/*.cjs"
    ],
    "coverageReporters": [
      "lcov",
      "cobertura",
      "text"
    ]
  }
}
